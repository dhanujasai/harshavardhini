#include <stdio.h>

int main()
{
    int  i,count=0,a[10],n;
    printf("enter array size");
    scanf("%d",&n);
    printf("enter array elements");
    for(i=0;i<n;i++)
    scanf("%d",&a[i]);
    for(i=0;i<n;i++)
    {
     if(a[i]!=0)
    {
        printf("%d",a[i]);
    }
    }
    for(i=0;i<n;i++)
    {
        if(count==a[i])
        {
            printf("%d",a[i]);
            
        }
    }
    
    

    return 0;
}
